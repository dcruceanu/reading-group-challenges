{% for flashMessage in app.session.flashbag.get('notice') %}
    <div class="flash-notice">
        {{ flashMessage }}
    </div>
{% endfor %}

{{ form_start(form) }}
{{ form_errors(form) }}
    <div>
        {{ form_label(form.url, 'Url') }}
        {{ form_errors(form.url) }}
        {{ form_widget(form.url) }}
    </div>
{{ form_end(form) }}
